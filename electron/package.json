{
  "name": "electron",
  "version": "1.0.0",
  "main": "main.js",
  "license": "MIT",
  "scripts": {
    "start:electron": "NODE_ENV=development && electron .",
    "build:clean": "rm -rf build",
    "prebuild": "yarn build:clean && node scripts/generate-buildno.js",
    "start": "NODE_ENV=dev webpack serve --mode development",
    "staging": "NODE_ENV=production webpack serve",
    "copy:assets": "rsync -ax public/* build --exclude=*.html",
    "build": "NODE_ENV=production yarn prebuild && webpack build --mode production && yarn copy:assets",
    "sync:i18": "npx ts-node scripts/syncTranslations.ts",
    "i18": "yarn sync:i18",
    "i18:excel": "npx nolybab-js -a to-csv -i ./public/locales -o ./public/locales",
    "prepare": "husky install",
    "commit": "cz",
    "commit:signed": "cz -S",
    "win-start": "env NODE_ENV=production && webpack serve",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\" --ignore-path .gitignore --ignore-path .prettierignore"
  },
  "devDependencies": {
    "@babel/core": "^7.18.9",
    "@babel/preset-env": "^7.18.9",
    "@babel/preset-react": "^7.18.6",
    "@svgr/webpack": "^6.3.1",
    "autoprefixer": "^10.4.7",
    "babel-loader": "^8.2.5",
    "check-if-windows": "^1.0.0",
    "commitizen": "^4.2.5",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.1",
    "cz-conventional-changelog": "^3.3.0",
    "electron": "^24.3.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.1",
    "mini-css-extract-plugin": "^2.6.1",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "sass": "^1.54.0",
    "sass-loader": "^13.0.2",
    "style-loader": "^3.3.1",
    "tailwindcss": "^3.1.6",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.3",
    "workbox-webpack-plugin": "^6.5.4"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.0.6",
    "@chakra-ui/react": "^2.2.4",
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@tailwindcss/line-clamp": "^0.4.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/node": "^18.7.11",
    "axios": "^0.27.2",
    "chakra-dayzed-datepicker": "^0.2.2",
    "chakra-react-select": "^4.1.4",
    "date-fns": "^2.29.3",
    "dayzed": "^3.2.3",
    "formik": "^2.2.9",
    "framer-motion": "^6.5.1",
    "git-rev-sync": "^3.0.2",
    "i18next": "^21.9.1",
    "i18next-browser-languagedetector": "^6.1.5",
    "i18next-xhr-backend": "^3.2.2",
    "immutability-helper": "^3.1.1",
    "ion-sdk-js": "^1.8.2",
    "jsonrepair": "^3.0.1",
    "leaflet": "^1.9.2",
    "nolybab-js": "^1.0.4",
    "nprogress": "^0.2.0",
    "react": "^18.2.0",
    "react-datepicker": "^4.8.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
