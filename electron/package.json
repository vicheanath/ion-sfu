    "react-router-dom": "^6.3.0",
    "react-select": "^5.7.0",
    "react-table": "^7.8.0",
    "tailwind-scrollbar": "^1.3.1",
    "typescript": "^4.7.4",
    "uuid": "^8.3.2",
    "web-vitals": "^2.1.4",
    "yup": "^0.32.11",
    "zustand": "^4.0.0-rc.1"
  }
}
